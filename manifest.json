{
    "manifest_version": 3,
    "name": "paymentlogs React x Vite Browser Extension Boilerplate",
    "version": "1.0.0",
    "description": "React x Vite Browser Extension Boilerplate written by paymentlogs",
    "permissions": [
        "activeTab",
        "tabs",
        "declarativeContent",
        "storage",
        "notifications",
        "proxy",
        "cookies",
        "contentSettings",
        "unlimitedStorage",
        "contextMenus",
        "webRequest"
    ],
    "icons": {},
    "background": {
        "service_worker": "packages/backend/background/background.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "paymentlogs React x Vite Extension Boilerplate"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "packages/backend/content_scripts/global.js"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [],
    "content_security_policy": {
        "extension_pages": "default-src 'self'"
    },
    "web_accessible_resources": []
}